{
    "name": "backend-test",
    "version": "1.0.0",
    "description": "Basic setup to for the backend engineer test",
    "main": "index.js",
    "scripts": {
        "start": "node --env-file=.env dist/index.js",
        "start:dev": "NODE_OPTIONS='--import tsx' --env-file=.env src/index.ts",
        "prestart": "npm run build",
        "build": "tsc",
        "test": "set DEBUG=* && jest --testPathPattern=test --coverage"
    },
    "author": "Christoph Fey",
    "license": "ISC",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/cors": "^2.8.16",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.8",
        "@types/node": "^20.9.0",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "eslint": "^8.53.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "jest": "^29.7.0",
        "prettier": "^3.0.3",
        "ts-jest": "^29.1.1",
        "tsx": "^4.1.1",
        "typescript": "^5.2.2"
    }
}
